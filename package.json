    "build:sketch": "webpack",
    "package-tarball": "npm run build && npm pack"
  },
  "devDependencies": {
    "@sketch-hq/sketch-assistant-utils": "6.0.0",
    "@types/jest": "26.0.4",
    "@types/node": "14.0.22",
    "jest": "26.1.0",
    "node-fetch": "2.6.0",
    "prettier": "2.0.5",
    "ts-jest": "26.1.2",
    "typescript": "3.9.6",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12"
  },
  "dependencies": {
    "@sketch-hq/sketch-assistant-types": "6.0.0"
  }
}
